{
    "name": "evotodi/seed-bundle",
    "description": "Seeder for symfony 6/7",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "evotodi",
            "email": "evotodi@gmail.com"
        },
        {
            "name": "Soyuka",
            "email": "soyuka@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/console": "^6.0|^7.0",
        "symfony/finder": "^6.0|^7.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.0",
        "symfony/yaml": "^6.0|^7.0",
        "symfony/doctrine-bridge": "^6.0|^7.0",
        "webmozart/glob": "^4.5"
    },
    "require-dev": {
        "symfony/config": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/phpunit-bridge": "^6.0|^7.0"

    },
    "autoload": {
        "psr-4": {
            "Evotodi\\SeedBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Evotodi\\SeedBundle\\Tests\\": "tests/"
        }
    }
